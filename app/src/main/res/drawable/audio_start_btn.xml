<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="48dp" android:height="48dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/background" />
            <stroke android:width="1dp" android:color="@color/text10"/>
        </shape>
    </item>

    <item android:width="30dp" android:height="30dp"
        android:left="9dp"
        android:right="9dp"
        android:top="9dp"
        android:bottom="9dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/primary20" />
        </shape>
    </item>
</layer-list>
