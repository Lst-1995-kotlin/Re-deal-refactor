<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="48dp" android:height="48dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/background"/>
            <stroke android:width="1dp" android:color="@color/text10"/>
        </shape>
    </item>

    <item android:width="24dp" android:height="24dp"
        android:left="12dp"
        android:right="12dp"
        android:top="12dp"
        android:bottom="12dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/primary20" />
            <corners android:radius="6dp"/>
        </shape>
    </item>
</layer-list>
